{
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      { "src": "/contacto", "dest": "/contacto.html" },
      { "src": "/miembros", "dest": "/miembros.html" },
      { "src": "/dashboard", "dest": "/dashboard.html" },
      { "src": "/reportes", "dest": "/reportes.html" },
      { "src": "/suscripciones", "dest": "/suscripciones.html" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  